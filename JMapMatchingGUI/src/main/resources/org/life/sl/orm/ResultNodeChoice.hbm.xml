<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.life.sl.orm">

	<class name="ResultNodeChoice" table="resultnodechoice">

		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="routeID"></property>
		<property name="sourceRouteID"></property>
		<property name="respondentID"></property>
		<property type="int" name="i"></property>
		<property type="int" name="nodeID"></property>
		<property type="int" name="edgeID"></property>
		<property type="boolean" name="selected"></property>
		<property type="float" name="dist"></property>
		<property type="float" name="angle"></property>
		<property type="float" name="angleToDest"></property>
		<property name="envType"></property>
		<property name="cykType"></property>
		<property type="float" name="groenM"></property>
		<property type="float" name="groenPct"></property>
		<property type="short" name="nPts"></property>
		<property type="short" name="nChoices"></property>

	</class>

</hibernate-mapping>
